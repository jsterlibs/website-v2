{
  "type": "ul",
  "class": "flex flex-col gap-4",
  "foreach": [
    {
      "utility": "get",
      "parameters": ["props", "blogPosts"]
    },
    {
      "type": "li",
      "children": [
        {
          "type": "Link",
          "bindToProps": {
            "children": {
              "utility": "get",
              "parameters": ["props", "title"]
            },
            "href": {
              "utility": "get",
              "parameters": ["props", "slug"]
            }
          }
        }
      ]
    }
  ]
}
