{
  "type": "ul",
  "foreach": [{ "utility": "get", "parameters": ["props", "categories"] }, {
    "type": "li",
    "class": "flex flex-col md:flex-row md:gap-4 border-b last:border-b-0",
    "children": [
      {
        "type": "h2",
        "children": {
          "utility": "get",
          "parameters": ["props", "title"]
        },
        "class": "px-4 py-2 md:w-1/6 md:text-right md:border-r-8 md:border-fuchsia-600 font-medium"
      },
      {
        "type": "ul",
        "class": "flex flex-wrap p-4 md:w-5/6 gap-4 font-light",
        "foreach": [
          {
            "utility": "get",
            "parameters": ["props", "children"]
          },
          {
            "type": "li",
            "class": "inline",
            "children": [
              {
                "type": "Link",
                "bindToProps": {
                  "children": {
                    "utility": "get",
                    "parameters": ["props", "title"]
                  },
                  "href": {
                    "utility": "get",
                    "parameters": ["props", "url"]
                  }
                }
              }
            ]
          }
        ]
      }
    ]
  }]
}
