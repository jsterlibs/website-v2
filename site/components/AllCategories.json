{
  "element": "ul",
  "foreach": ["props.categories", {
    "element": "li",
    "class": "flex flex-col md:flex-row md:gap-4 border-b last:border-b-0",
    "children": [
      {
        "element": "h2",
        "__children": "props.title",
        "class": "px-4 py-2 md:w-1/6 md:text-right md:border-r-8 md:border-fuchsia-600 font-medium"
      },
      {
        "element": "ul",
        "class": "flex flex-wrap p-4 md:w-5/6 gap-4 font-light",
        "foreach": [
          "props.children",
          {
            "element": "li",
            "class": "inline",
            "children": [
              {
                "element": "Link",
                "props": {
                  "__children": "props.title",
                  "__href": "props.url"
                }
              }
            ]
          }
        ]
      }
    ]
  }]
}
