{
  "element": "div",
  "attributes": {
    "x-state": "'{ libraries: [] }'"
  },
  "children": [
    {
      "element": "div",
      "class": "flex gap-1",
      "children": [
        {
          "element": "button",
          "children": "Sort libraries (asc.)",
          "attributes": {
            "onclick": "setState(state => ({ libraries: state.libraries.slice().sort((a, b) => a.title.localeCompare(b.title)) }))"
          }
        },
        {
          "element": "button",
          "children": "Sort libraries (desc.)",
          "attributes": {
            "onclick": "setState(state => ({ libraries: state.libraries.slice().sort((a, b) => b.title.localeCompare(a.title)) }))"
          }
        }
      ]
    },
    {
      "element": "div",
      "debug_attributes": {
        "x": "JSON.stringify(state.libraries, null, 2)"
      }
    },
    {
      "element": "ul",
      "class": "grid md:grid-cols-2 lg:grid-cols-3 gap-4",
      "attributes": {
        "x-each": "state.libraries",
        "x-ssr": ""
      },
      "__children": [
        {
          "element": "li",
          "class": "my-4 flex flex-col gap-4 w-full bg-gray-200 rounded-lg p-4",
          "attributes": {
            "x-template": ""
          },
          "children": [
            {
              "element": "Link",
              "class": "bg-gray-100 hover:bg-gray-300 text-gray-800 p-4 border-fuchsia-600 rounded-lg",
              "__children": "title",
              "attributes": {
                "__href": "url",
                "x": "state.value.title",
                "x-attr": "",
                "@href": "state.value.url"
              }
            },
            {
              "element": "div",
              "class": "flex justify-between",
              "children": [
                {
                  "element": "div",
                  "class": "flex gap-1",
                  "children": [
                    {
                      "element": "span",
                      "children": "Stars:"
                    },
                    {
                      "element": "span",
                      "__children": "library.stargazers"
                    }
                  ]
                },
                {
                  "element": "Link",
                  "children": "GitHub page",
                  "attributes": {
                    "__href": "library.links.github"
                  }
                }
              ]
            },
            {
              "element": "Tags",
              "__bind": "library.tags"
            }
          ]
        }
      ]
    }
  ]
}
