{
  "type": "div",
  "attributes": {
    "x-state": "'{ libraries: [] }'"
  },
  "children": [
    {
      "type": "div",
      "class": "flex gap-1",
      "todo_children": [
        {
          "type": "button",
          "children": "Sort libraries (asc.)",
          "attributes": {
            "onclick": "setState(state => ({ libraries: state.libraries.slice().sort((a, b) => a.title.localeCompare(b.title)) }))"
          }
        },
        {
          "type": "button",
          "children": "Sort libraries (desc.)",
          "attributes": {
            "onclick": "setState(state => ({ libraries: state.libraries.slice().sort((a, b) => b.title.localeCompare(a.title)) }))"
          }
        }
      ]
    },
    {
      "type": "ul",
      "class": "grid md:grid-cols-2 lg:grid-cols-3 gap-4",
      "attributes": {
        "x-each": "state.libraries",
        "x-ssr": ""
      },
      "foreach": [
        {
          "utility": "get",
          "parameters": ["props", "libraries"]
        },
        {
          "type": "li",
          "class": "my-4 flex flex-col gap-4 w-full bg-gray-200 rounded-lg p-4",
          "attributes": {
            "x-template": ""
          },
          "children": [
            {
              "type": "Link",
              "bindToProps": {
                "children": {
                  "utility": "get",
                  "parameters": ["props", "name"]
                },
                "href": {
                  "utility": "concat",
                  "parameters": ["/library/", {
                    "utility": "get",
                    "parameters": ["props", "id"]
                  }]
                }
              },
              "props": {
                "class": "bg-gray-100 hover:bg-gray-300 text-gray-800 p-4 border-fuchsia-600 rounded-lg",
                "x": "state.value.name",
                "@href": "state.value.url"
              }
            },
            {
              "type": "div",
              "class": "flex justify-between",
              "children": [
                {
                  "visibleIf": [{
                    "utility": "get",
                    "parameters": ["props", "stargazers"]
                  }],
                  "type": "div",
                  "class": "flex gap-1",
                  "children": [
                    {
                      "type": "span",
                      "children": "Stars:"
                    },
                    {
                      "type": "span",
                      "children": {
                        "utility": "get",
                        "parameters": ["props", "stargazers"]
                      },
                      "attributes": {
                        "x": "state.value.stargazers",
                        "x-attr": ""
                      }
                    }
                  ]
                },
                {
                  "visibleIf": [{
                    "utility": "get",
                    "parameters": ["props", "links.site"]
                  }],
                  "type": "Link",
                  "bindToProps": {
                    "href": {
                      "utility": "get",
                      "parameters": ["props", "links.site"]
                    }
                  },
                  "props": {
                    "children": "Homepage",
                    "@href": "state.value.links.site"
                  }
                },
                {
                  "visibleIf": [{
                    "utility": "get",
                    "parameters": ["props", "links.github"]
                  }],
                  "type": "Link",
                  "bindToProps": {
                    "href": {
                      "utility": "get",
                      "parameters": ["props", "links.github"]
                    }
                  },
                  "props": {
                    "children": "GitHub page",
                    "@href": "state.value.links.github"
                  }
                }
              ]
            },
            {
              "type": "Tags",
              "bindToProps": {
                "tags": {
                  "utility": "get",
                  "parameters": ["props", "tags"]
                }
              }
            }
          ]
        }
      ]
    }
  ]
}
