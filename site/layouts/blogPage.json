{
  "element": "BaseLayout",
  "props": {
    "content": {
      "element": "div",
      "class": "p-4 mx-auto max-w-5xl prose lg:prose-xl",
      "children": [
        {
          "element": "H1",
          "props": {
            "__children": "context.match.title"
          }
        },
        {
          "element": "p",
          "==children": "utilities.markdown(context.match.body).content"
        }
      ]
    }
  }
}
