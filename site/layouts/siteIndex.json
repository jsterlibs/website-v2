{
  "type": "BaseLayout",
  "props": {
    "content": {
      "type": "div",
      "class": "p-4",
      "children": [
        {
          "type": "div",
          "class": "mx-auto max-w-5xl",
          "children": [
            {
              "type": "Subscribe"
            },
            {
              "type": "div",
              "class": "mx-auto my-4 p-2 bg-yellow-100 prose",
              "props": {
                "content": "**Note:** The new site is still in beta. Please [use the issue tracker for bug reports and feature requests](https://github.com/jsterlibs/website-v2/issues)."
              },
              "children": {
                "utility": "markdown",
                "parameters": [
                  { "utility": "get", "parameters": ["props", "content"] }
                ]
              }
            },
            {
              "type": "AllCategories",
              "bindToProps": {
                "categories": {
                  "utility": "get",
                  "parameters": ["context", "parentCategories"]
                }
              }
            }
          ]
        }
      ]
    }
  }
}
