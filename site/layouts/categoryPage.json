{
  "type": "BaseLayout",
  "props": {
    "content": {
      "type": "div",
      "class": "p-4 mx-auto max-w-5xl flex flex-col gap-4",
      "children": [
        {
          "type": "H1",
          "bindToProps": {
            "children": {
              "utility": "get",
              "parameters": ["context", "match.title"]
            }
          }
        },
        {
          "type": "AllLibraries",
          "bindToProps": {
            "libraries": {
              "utility": "get",
              "parameters": ["context", "match.libraries"]
            }
          }
        }
      ]
    }
  }
}
