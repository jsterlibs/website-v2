{
  "meta": {
    "__title": "category.title",
    "__description": "category.description"
  },
  "dataSources": [{ "name": "categories", "matchBy": "id" }],
  "page": [
    {
      "component": "main-navigation"
    },
    {
      "element": "main",
      "class": "py-4 mx-auto max-w-5xl",
      "children": [
        {
          "component": "h1",
          "class": "mb-4",
          "__children": "category.title"
        },
        {
          "component": "all-libraries",
          "__bind": "category.libraries"
        }
      ]
    },
    {
      "component": "main-footer"
    }
  ]
}
