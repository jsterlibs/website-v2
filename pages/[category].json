{
  "meta": {
    "__title": "match.title",
    "__description": "match.description"
  },
  "matchBy": { "dataSource": "categories", "field": "id" },
  "dataSources": [{ "name": "categories" }],
  "page": [
    {
      "component": "mainNavigation"
    },
    {
      "element": "main",
      "class": "py-4 mx-auto max-w-5xl",
      "children": [
        {
          "component": "h1",
          "class": "mb-4",
          "__children": "match.title"
        },
        {
          "component": "allLibraries",
          "__bind": "match.libraries"
        }
      ]
    },
    {
      "component": "mainFooter"
    }
  ]
}
