{
  "all-categories": [
    {
      "element": "ul",
      "__bind": "./data/parent-categories.json",
      "__children": [
        {
          "element": "flex",
          "as": "li",
          "class": "gap-4",
          "children": [
            {
              "element": "h2",
              "__children": "title",
              "class": "px-4 py-2 w-1/6 text-right border-r-8 border-fuchsia-600"
            },
            {
              "element": "ul",
              "class": "p-4 w-5/6",
              "__foreach": "children",
              "__render": [
                {
                  "element": "flex",
                  "as": "li",
                  "children": [
                    {
                      "element": "a",
                      "__children": "title",
                      "attributes": {
                        "__href": "url"
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "subscribe": [
    {
      "element": "div",
      "children": "Subscription widget goes here"
    }
  ],
  "navigation": [
    {
      "element": "a",
      "children": "Blog",
      "attributes": {
        "href": "/blog/"
      }
    },
    {
      "element": "a",
      "children": "Catalog",
      "attributes": {
        "href": "/catalog/"
      }
    },
    {
      "element": "a",
      "children": "About",
      "attributes": {
        "href": "/about/"
      }
    },
    {
      "element": "a",
      "children": "Add library",
      "attributes": {
        "href": "/add-library/"
      }
    }
  ]
}
